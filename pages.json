{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/home",
		"style": {
			"navigationBarTitleText": "美食广场",
			"enablePullDownRefresh": false
		}
	}, {
		"path": "pages/mine",
		"style": {
			"navigationBarTitleText": "我的",
			"enablePullDownRefresh": false
		}
	}],
	"subPackages": [{
		"root": "pages/myPackageA",
		"pages": [{
				"path": "shopping/shopping",
				"style": {
					"navigationBarTitleText": "商店",
					"enablePullDownRefresh": false,
					"navigationStyle": "custom"
				}
			}, {
				"path": "mine/editAddress/editAddress",
				"style": {
					"navigationBarTitleText": "",
					"enablePullDownRefresh": false,
					"navigationStyle": "custom"
				}

			}, {
				"path": "mine/address/address",
				"style": {
					"navigationBarTitleText": "收货地址",
					"enablePullDownRefresh": false,
					"navigationStyle": "custom"
				}
			},
			{
				"path": "index/index",
				"style": {
					"navigationBarTitleText": "收货地址",
					"enablePullDownRefresh": false
				}
			}, {
				"path": "shopping/settlement/settlement",
				"style": {
					"navigationBarTitleText": "结算",
					"enablePullDownRefresh": false,
					"navigationStyle": "custom"
				}
			}, {
				"path": "orders/orders",
				"style": {
					"navigationBarTitleText": "订单",
					"enablePullDownRefresh": false,
					"navigationStyle": "custom"
				}
			}
		]
	}],

	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uView",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#FFFFFF"
			//#ifdef H5
			,
		"navigationStyle": "custom"
		//#endif
	},
	"tabBar": {
		"color": "#909399",
		"selectedColor": "#303133",
		"backgroundColor": "#FFFFFF",
		"borderStyle": "black",
		"list": [{
				"pagePath": "pages/home",
				"iconPath": "static/home_checked.png",
				"selectedIconPath": "static/home_checked.png",
				"text": "美食广场"
			},
			{
				"pagePath": "pages/mine",
				"iconPath": "static/home_checked.png",
				"selectedIconPath": "static/home_checked.png",
				"text": "我的"
			}

		]
	}
}
